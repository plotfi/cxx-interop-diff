  typealias AsyncIterator = AsyncThrowingPrefixWhileSequence<Base>.Iterator
  /// The iterator that produces elements of the prefix-while sequence.
  struct Iterator : AsyncIteratorProtocol {
    @usableFromInline
    var predicateHasFailed: Bool
    @usableFromInline
